@model IEnumerable<dynamic>

<h2>Top 10 Customers by Sales</h2>

<table class="table table-striped">
    <thead>
    <tr>
        <th>Customer Name</th>
        <th>Total Sales</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.CustomerName</td>
            <td>@item.TotalSales</td>
        </tr>
    }
    </tbody>
</table>
